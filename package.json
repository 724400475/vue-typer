{
  "name": "vue-typer",
  "version": "1.0.0",
  "description": "Vue component that simulates a user typing, selecting, and deleting text.",
  "main": "dist/vue-typer.min.js",
  "scripts": {
    "dev": "NODE_ENV='development' webpack-dev-server --config build/webpack.config.dev.js --hot --inline --open --compress",
    "dev:lan": "npm run dev -- --host 0.0.0.0",
    "demo": "NODE_ENV='production' webpack --config build/webpack.config.demo.js",
    "build": "NODE_ENV='production' webpack --config build/webpack.config.prod.js",
    "test": "NODE_ENV='testing' echo \"Error: no test specified\" && exit 1",
    "lint": "NODE_ENV='production' eslint --format node_modules/eslint-friendly-formatter --ext .js,.vue src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cngu/vue-typer.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "vue.js",
    "component",
    "typer",
    "typewriter",
    "type",
    "auto"
  ],
  "author": "Chris Nguyen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cngu/vue-typer/issues"
  },
  "homepage": "https://github.com/cngu/vue-typer#readme",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^3.12.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^1.7.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "html-webpack-plugin": "^2.24.1",
    "webpack": "^2.1.0-beta.28",
    "webpack-dev-server": "^2.2.0-rc.0",
    "webpack-merge": "^1.1.2"
  },
  "dependencies": {
    "vue": "^2.1.6"
  }
}
